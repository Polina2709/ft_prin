    printf("-+10  ||%-+10c|\n", 'a');
    ft_printf("-+10  ||%-+10c|\n", 'a');
    printf("\n", 'a');
    printf("-+0   ||%-+010c|\n", 'a');
    ft_printf("-+0   ||%-+010c|\n", 'a');
    printf("\n", 'a');
    printf("-10   ||%-10c|\n", 'a');
    ft_printf("-10   ||%-10c|\n", 'a');
    printf("\n", 'a');
    printf("-10.4 ||%-10.4c|\n", 'a');
    ft_printf("-10.4 ||%-10.4c|\n", 'a');
    printf("\n", 'a');
    printf("010   ||%010c|\n", 'a');
    ft_printf("010   ||%010c|\n", 'a');
    printf("\n", 'a');
    printf("+10.4 ||%-10.4c|\n", 'a');
    ft_printf("+10.4 ||%-10.4c|\n", 'a');
    printf("\n", 'a');
    printf("+10   ||%-10c|\n", 'a');
    ft_printf("+10   ||%-10c|\n", 'a');
    printf("\n", 'a');
    printf("-010  ||%010c|\n", 'a');
    ft_printf("-010  ||%010c|\n", 'a');
    printf("\n", 'a');
    printf("+010  ||%010c|\n", 'a');
    ft_printf("+010  ||%010c|\n", 'a');
    printf("\n", 'a');

    printf("-+     ||%-+c|\n", 'a');
    ft_printf("-+     ||%-+c|\n", 'a');
    printf("\n", 'a');
    printf("-10    ||%-10c|\n", 'a');
    ft_printf("-10    ||%-10c|\n", 'a');
    printf("\n", 'a');
    printf("-0     ||%-0c|\n", 'a');
    ft_printf("-0     ||%-0c|\n", 'a');
    printf("\n", 'a');
    printf("-+0    ||%-+0c|\n", 'a');
    ft_printf("-+0    ||%-+0c|\n", 'a');
    printf("\n", 'a');
    printf("-+10   ||%-+10c|\n", 'a');
    ft_printf("-+10   ||%-+10c|\n", 'a');
    printf("\n", 'a');
    printf("-+010  ||%-+010c|\n", 'a');
    ft_printf("-+010  ||%-+010c|\n", 'a');
    printf("\n", 'a');
    printf("-0     ||%-0c|\n", 'a');
    ft_printf("-0     ||%-0c|\n", 'a');
    printf("\n", 'a');
    printf("-010   ||%-010c|\n", 'a');
    ft_printf("-010   ||%-010c|\n", 'a');
    printf("\n", 'a');

    printf("+-     ||%+-c|\n", 'a');
    ft_printf("+-     ||%+-c|\n", 'a');
    printf("\n", 'a');
    printf("+10    ||%+10c|\n", 'a');
    ft_printf("+10    ||%+10c|\n", 'a');
    printf("\n", 'a');
